{
  "name": "eLAN MQTT",
  "version": "1.15",
  "slug": "elan2mqtt",
  "description": "MQTT gateway for eLAN",
  "init": false,
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "before",
  "boot": "auto",
  "options": {
    "eLanURL": "http://192.168.1.99",
    "MQTTserver": "192.168.171.207",
    "mqtt_user": "elan",
    "mqtt_pass": "elan",
    "username": "admin",
    "password": "elkoep",
    "log_level": "info",
    "disable_autodiscovery": false,
    "mqtt_id": "elan",
    "publish_interval": 300,
    "discover_interval": 600,
    "socket_interval": 0
  },
  "schema": {
    "eLanURL": "str",
    "MQTTserver": "str",
    "username": "str",
    "password": "str",
    "log_level": "match(^(trace|debug|info|notice|warning|error|fatal)$)",
    "disable_autodiscovery": "bool?",
    "mqtt_id": "str?"
  },
  "logging": {
    "formatter": "%(asctime)s %(filename)s:%(lineno)d %(coproc)s/%(levelname)s - %(message)s",
    "log_level": "debug"
  }
}
